<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".ui.homefragment.HomeFragment">

    <androidx.swiperefreshlayout.widget.SwipeRefreshLayout
        android:id="@+id/pullToRefresh"
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <androidx.recyclerview.widget.RecyclerView
            android:id="@+id/rv_timbri"
            android:name="com.octotelematics.digitaldriverwhitelabel.fragments.fleetmanagement.ChangeDriverFragment"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            app:layoutManager="LinearLayoutManager"
            android:layout_marginBottom="20dp"
            tools:context=".ui.homefragment.HomeFragment"
            tools:listitem="@layout/timbri_item" />
    </androidx.swiperefreshlayout.widget.SwipeRefreshLayout>
</FrameLayout>